@top Program {expression*}

expression {
    Test | 
    Increment
}

inner {
    Identifier |
    Internal
}

Test {"{" inner* "}"}
Increment {@specialize <Identifier, "Increment">}
Internal {@specialize <Identifier, "Internal">}

@tokens {
    Identifier { $[a-zA-Z_]+ }
    space { $[ \t\n\r]+ }
}

@skip { space }
